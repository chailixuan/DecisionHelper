<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/hasnamed_name_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="name"
        android:textSize="20dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="5dp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/hasnamed_data_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="data"
        android:textSize="17dp"
        app:layout_constraintTop_toBottomOf="@+id/hasnamed_name_tv"
        android:layout_marginTop="5dp"
        app:layout_constraintLeft_toLeftOf="@+id/hasnamed_name_tv"
        app:layout_constraintRight_toLeftOf="@+id/hasnamed_ib"/>
    <ImageButton
        android:id="@+id/hasnamed_ib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_hasnamedgoto"
        android:background="@android:color/transparent"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toTopOf="@+id/hasnamed_name_tv"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="15dp"
        />

</android.support.constraint.ConstraintLayout>